    "eslint-config-prettier": "10.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "14.6.1",
    "nx": "21.5.3",
    "postcss": "^8.4.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^4.0.8",
    "ts-jest": "29.4.4",
