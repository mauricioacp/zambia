    "tslib": "^2.3.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.33.1",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "overrides": {
    "@supabase/supabase-js": {
      "@supabase/auth-js": "2.61.0"
    }
  }
}
