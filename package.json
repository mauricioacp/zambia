    "@nx/storybook": "20.8.1",
    "@nx/web": "20.8.1",
    "@nx/workspace": "20.8.1",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "19.2.1",
    "@smarttools/eslint-plugin-rxjs": "^1.0.18",
    "@storybook/addon-essentials": "8.6.11",
    "@storybook/addon-interactions": "8.6.11",
