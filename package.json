{
  "name": "@zambia/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npx nx run zambia:storybook:ci",
    "config": "ts-node ./apps/zambia/set-env.ts",
    "config:dev": "cross-env NODE_ENV=development ts-node ./apps/zambia/set-env.ts",
    "config:prod": "cross-env NODE_ENV=production ts-node ./apps/zambia/set-env.ts",
    "serve": "npm run config:dev && npx nx run zambia:serve:development",
    "build": "npx nx run zambia:build:development",
    "build:prod": "npm run config:prod && npx nx run zambia:build:production",
    "docker:dev": "docker-compose -f docker-compose.dev.yaml up",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yaml up --build",
    "docker:prod": "docker-compose up -d",
    "docker:prod:build": "docker-compose up --build -d",
    "supabase:gen:types:local": "npx supabase gen types typescript --local > libs/shared/types-supabase/src/lib/types/supabase.type.ts",
    "dev": "npx nx run zambia:serve:development",
    "test": "npx nx test zambia",
    "e2e": "npx nx e2e zambia",
    "cm": "cz",
    "prettier:staged": "npx pretty-quick --staged",
    "prepare": "husky",
    "graph": "npx nx graph",
    "lint:all": "npx nx run-many --target=lint --all",
    "lint:affected": "npx nx affected --target=lint",
    "test:all": "npx nx run-many --target=test --all",
    "test:affected": "npx nx affected --target=test",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,html,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,html,css,scss,json,md}\"",
    "reset": "npx nx reset"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.1",
    "@angular/cdk": "^19.2.2",
    "@angular/common": "19.2.1",
    "@angular/compiler": "19.2.1",
    "@angular/core": "19.2.1",
    "@angular/forms": "19.2.1",
    "@angular/platform-browser": "19.2.1",
    "@angular/platform-browser-dynamic": "19.2.1",
    "@angular/platform-server": "19.2.1",
    "@angular/router": "19.2.1",
    "@angular/ssr": "19.2.1",
    "@ngx-translate/core": "^16.0.4",
    "@supabase/supabase-js": "^2.49.4",
    "@taiga-ui/addon-charts": "^4.35.0",
    "@taiga-ui/addon-mobile": "^4.35.0",
    "@taiga-ui/addon-table": "^4.35.0",
    "@taiga-ui/cdk": "^4.35.0",
    "@taiga-ui/core": "^4.35.0",
    "@taiga-ui/event-plugins": "^4.0.2",
    "@taiga-ui/icons": "^4.35.0",
    "@taiga-ui/kit": "^4.35.0",
    "@taiga-ui/layout": "^4.35.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "rxjs": "~7.8.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.1",
