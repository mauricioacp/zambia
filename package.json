{
  "name": "@zambia/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "config": "ts-node ./apps/zambia/set-env.ts",
    "config:dev": "cross-env NODE_ENV=development ts-node ./apps/zambia/set-env.ts",
    "config:prod": "cross-env NODE_ENV=production ts-node ./apps/zambia/set-env.ts",
    "serve": "npm run config:dev && npx nx run zambia:serve:development",
    "build": "npx nx run zambia:build:development",
    "build:prod": "npm run config:prod && npx nx run zambia:build:production",
    "build:analyze": "npx nx run zambia:build:production --stats-json && npx webpack-bundle-analyzer dist/apps/zambia/stats.json",
    "docker:dev": "docker-compose -f docker-compose.dev.yaml up",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yaml up --build",
    "docker:prod": "docker-compose up -d",
    "docker:prod:build": "docker-compose up --build -d",
    "supabase:gen:types:local": "npx supabase gen types typescript --local > libs/shared/types-supabase/src/lib/types/supabase.type.ts",
    "dev": "npx nx run zambia:serve:development",
    "test": "npx nx test zambia",
    "e2e": "npx nx e2e zambia",
    "cm": "cz",
    "prettier:staged": "npx pretty-quick --staged",
    "prepare": "husky",
    "graph": "npx nx graph",
    "lint:all": "npx nx run-many --target=lint --all",
    "lint:affected": "npx nx affected --target=lint",
    "test:all": "npx nx run-many --target=test --all",
    "test:affected": "npx nx affected --target=test",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,html,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,html,css,scss,json,md}\"",
    "reset": "npx nx reset"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.2.4",
    "@angular/cdk": "20.2.5",
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-browser-dynamic": "20.2.4",
    "@angular/platform-server": "20.2.4",
    "@angular/router": "20.2.4",
    "@angular/ssr": "20.2.2",
    "@ngx-translate/core": "^16.0.4",
    "@sentry/browser": "^9.30.0",
    "@supabase/supabase-js": "^2.49.4",
    "@taiga-ui/addon-charts": "^4.39.1",
    "@taiga-ui/addon-mobile": "^4.42.0",
    "@taiga-ui/addon-table": "^4.35.0",
    "@taiga-ui/cdk": "^4.35.0",
    "@taiga-ui/core": "^4.35.0",
    "@taiga-ui/event-plugins": "^4.0.2",
    "@taiga-ui/icons": "^4.35.0",
    "@taiga-ui/kit": "^4.35.0",
    "@taiga-ui/layout": "^4.35.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "rxjs": "~7.8.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.2.2",
    "@angular-devkit/core": "20.2.2",
    "@angular-devkit/schematics": "20.2.2",
    "@angular-eslint/template-parser": "20.3.0",
    "@angular/cli": "~20.2.0",
    "@angular/compiler-cli": "20.2.4",
    "@angular/language-service": "20.2.4",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "21.5.3",
    "@nx/devkit": "21.5.3",
    "@nx/eslint": "21.5.3",
    "@nx/eslint-plugin": "21.5.3",
    "@nx/jest": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/playwright": "21.5.3",
    "@nx/web": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "20.2.2",
    "@smarttools/eslint-plugin-rxjs": "^1.0.18",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
